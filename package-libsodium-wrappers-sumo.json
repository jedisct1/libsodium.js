{
  "name": "libsodium-wrappers-sumo",
  "version": "0.8.2",
  "description": "The Sodium cryptographic library compiled to pure JavaScript (wrappers, sumo variant)",
  "main": "dist/modules-sumo/libsodium-wrappers.js",
  "module": "dist/modules-sumo-esm/libsodium-wrappers.mjs",
  "types": "dist/modules-sumo/libsodium-wrappers.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/modules-sumo-esm/libsodium-wrappers.d.mts",
        "default": "./dist/modules-sumo-esm/libsodium-wrappers.mjs"
      },
      "require": {
        "types": "./dist/modules-sumo/libsodium-wrappers.d.ts",
        "default": "./dist/modules-sumo/libsodium-wrappers.js"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist/modules-sumo/libsodium-wrappers.js",
    "dist/modules-sumo/libsodium-wrappers.d.ts",
    "dist/modules-sumo-esm/libsodium-wrappers.mjs",
    "dist/modules-sumo-esm/libsodium-wrappers.d.mts",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jedisct1/libsodium.js.git"
  },
  "dependencies": {
    "libsodium-sumo": "^0.8.0"
  },
  "devDependencies": {
    "terser": "^5.44.1"
  },
  "keywords": [
    "crypto",
    "sodium",
    "libsodium",
    "nacl",
    "chacha20",
    "poly1305",
    "curve25519",
    "ed25519",
    "blake2",
    "siphash",
    "argon2",
    "ecc"
  ],
  "author": "Ahmad Ben Mrad (@BatikhSouri)",
  "contributors": [
    "Frank Denis (@jedisct1)",
    "Ryan Lester (@buu700)"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jedisct1/libsodium.js/issues"
  },
  "homepage": "https://github.com/jedisct1/libsodium.js",
  "browser": {
    "fs": false,
    "path": false,
    "stream": false,
    "crypto": false
  }
}
